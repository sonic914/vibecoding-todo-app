{
  "PoolName": "VibecodingTodoApp",
  "AutoVerifiedAttributes": ["email"],
  "Schema": [
    {
      "Name": "email",
      "Required": true,
      "Mutable": true
    }
  ],
  "Policies": {
    "PasswordPolicy": {
      "MinimumLength": 8,
      "RequireUppercase": true,
      "RequireLowercase": true,
      "RequireNumbers": true,
      "RequireSymbols": false
    }
  }
}
